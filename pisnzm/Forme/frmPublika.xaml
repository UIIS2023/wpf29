<Window x:Class="pisnzm.Frame.frmPublika"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pisnzm.Frame"
        mc:Ignorable="d"
        Title="frmPublika" Height="350" Width="300">
    <Grid>
        <Button x:Name="btnSacuvaj" Content="Sacuvaj" HorizontalAlignment="Left" Margin="59,266,0,0" VerticalAlignment="Top" Click="btnSacuvaj_Click"/>
        <Button x:Name="btnOtkazi" Content="Otkazi" HorizontalAlignment="Left" Margin="199,266,0,0" VerticalAlignment="Top" Click="btnOtkazi_Click"/>
        <Label Content="Koncentracija publike: " HorizontalAlignment="Left" Margin="14,59,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtKoncentracijaPublike" HorizontalAlignment="Left" Margin="159,63,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="KK: " HorizontalAlignment="Left" Margin="14,113,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cmbKK" HorizontalAlignment="Left" Margin="98,115,0,0" VerticalAlignment="Top" Width="120" SelectedValuePath="KKID"/>

    </Grid>
</Window>
